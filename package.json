{
  "name": "es6-vs-code-debug",
  "version": "1.0.0",
  "description": "This is my attempt at setting up Visual Studio Code to debug ES6",
  "main": "app.js",
  "scripts": {
    "a": "babel --presets env app.js --out-file proxified.js",
    "start": "./node_modules/.bin/babel-node app.js",
    "auto-start": "nodemon ./app.js localhost 8080",
    "build:lib": "BABEL_ENV=lib $(npm bin)/babel src/ --ignore=src/__mocks__ --out-dir=dist --source-maps",
    "build:bin": "BABEL_ENV=cli $(npm bin)/babel src/bin/ --out-dir=dist/bin --copy-files --source-maps",
    "build": "rm -rf dist/ && npm run build:lib && npm run build:bin",
    "watch:src": "npm run build:lib -- --watch",
    "watch:bin": "npm run build:bin -- --watch",
    "prepare": "npm run build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryanvgates/es6-vs-code-debug.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ryanvgates/es6-vs-code-debug/issues"
  },
  "homepage": "https://github.com/ryanvgates/es6-vs-code-debug#readme",
  "dependencies": {
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "debug": "^3.1.0",
    "express": "^4.16.2",
    "jade": "^1.11.0",
    "morgan": "^1.9.0",
    "serve-favicon": "^2.4.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^4.9.0",
    "lint": "^1.1.2",
    "nodemon": "^1.12.1"
  }
}
